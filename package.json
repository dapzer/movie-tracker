{
  "name": "movie-tracker",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@10.29.1+sha512.48dae233635a645768a3028d19545cacc1688639eeb1f3734e42d6d6b971afbf22aa1ac9af52a173d9c3a20c15857cfa400f19994d79a2f626fcc73fccda9bbc",
  "scripts": {
    "build": "dotenv -- turbo run build",
    "dev": "dotenv -- turbo run dev",
    "predev": "pnpm --filter=database run migrate",
    "lint": "turbo run lint",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postbuild": "dotenv -- turbo run postbuild",
    "db:push": "dotenv -- turbo db:push"
  },
  "dependencies": {
    "dotenv-cli": "7.4.4",
    "eslint": "9.34.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.19.0",
    "eslint-plugin-format": "1.0.1",
    "turbo": "2.5.6",
    "typescript": "5.9.2"
  }
}
